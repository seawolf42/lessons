{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.arrowParens": "avoid",
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.useEditorConfig": true,
  "files.exclude": {
    "env": true,
    "**/__pycache__": true,
    "**/.ipynb_checkpoints": true,
    "**/._*": true
  },
  "search.exclude": {
    "env": true,
    "**/__pycache__": true,
    "**/.ipynb_checkpoints": true,
    "**/._*": true
  }
}
